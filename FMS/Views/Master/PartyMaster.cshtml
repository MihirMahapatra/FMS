@{
    Layout = "_Layout";
    ViewData["Title"] = "Party Master";
}

<style>
    .cust-btn {
        margin-top: 32px !important;
    }
</style>


<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <div class="container-fluid">
            <div class="row mb-2">
                <div class="col-sm-6">
                    <h1></h1>
                </div>
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                    </ol>
                </div>
            </div>
        </div><!-- /.container-fluid -->
    </section>

    <!-- Main content -->
    <section class="content">
        <div class="container-fluid">
            <div class="card card-primary card-outline">
                <div class="card-header">
                    <h3 class="card-title">
                        <i class="fas fa-edit"></i>
                        Party Setup
                    </h3>
                </div>
                <div class="card-body">
                    <div id="loader" style="display:none;"></div>
                    <div class="row">
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Party Type</label>
                                <select class="form-control select2bs4" style="width: 100%;" name="ddnPartyTypeId" tabindex="1">
                                </select>
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>Party Name</label>
                                <input type="text" class="form-control" id="txtPartyName" name="PartyName" tabindex="2">
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Phone No</label>
                                <input type="text" class="form-control" id="txtPhoneNo" name="PhoneNo" tabindex="3">
                            </div>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>Email</label>
                                <input type="text" class="form-control" id="txtMail" name="Mail" tabindex="4">
                            </div>
                        </div>
                        <div class="col-md-3">
                            <div class="form-group">
                                <label>GST No</label>
                                <input type="text" class="form-control" id="txtGstNo" name="GstNo" tabindex="5">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>State</label>
                                <select class="form-control select2bs4" style="width: 100%;" id="stateId" name="ddnStateId" tabindex="6">
                                </select>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <button class="btn btn-primary btn-link cust-btn" data-toggle="modal" data-target="#modal-add-state" style="border: 0px;color: #fff; background-color:#337AB7; border-color: #3C8DBC; border-radius: 4px;"> <i class="fa-solid fa-plus"></i></button>
                            <button class="btn btn-primary btn-link cust-btn" data-toggle="modal" data-target="#modal-edit-state" id="btnStateEdit" style="border: 0px;color: #fff; background-color:#337AB7; border-color: #3C8DBC; border-radius: 4px;"><i class="fa-solid fa-edit"></i></button>
                            <button class="btn btn-primary btn-link cust-btn" id="btnStateDelete" style="border: 0px;color: #fff; background-color:#FF0000; border-color: #3C8DBC; border-radius: 4px;"><i class="fa-solid fa-trash-can"></i></button>
                        </div>
                        <div class="col-md-2">
                            <div class="form-group">
                                <label>City</label>
                                <select class="form-control select2bs4" style="width: 100%;" id="cityId" name="ddnCityId" tabindex="7" disabled>
                                </select>
                            </div>
                        </div>
                        <div class="col-md-2">
                            <button class="btn btn-primary btn-link cust-btn" data-toggle="modal" data-target="#modal-add-city" id="btnCityAdd" style="border: 0px;color: #fff; background-color:#337AB7; border-color: #3C8DBC; border-radius: 4px;"> <i class="fa-solid fa-plus"></i></button>
                            <button class="btn btn-primary btn-link cust-btn" data-toggle="modal" data-target="#modal-edit-city" id="btnCityEdit" style="border: 0px;color: #fff; background-color:#337AB7; border-color: #3C8DBC; border-radius: 4px;"><i class="fa-solid fa-edit"></i></button>
                            <button class="btn btn-primary btn-link cust-btn" id="btnCityDelete" style="border: 0px;color: #fff; background-color:#FF0000; border-color: #3C8DBC; border-radius: 4px;"><i class="fa-solid fa-trash-can"></i></button>
                        </div>
                        <div class="col-md-4">
                            <div class="form-group">
                                <label>Address</label>
                                <input type="text" class="form-control" id="txtAdress" name="Address" tabindex="8">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <button type="button" tabindex="9" class="btn btn-primary btn-party-create" style="float:right">Submit</button>
                        </div>
                        <div class="col-md-12 mt-3">
                            <button class="btn btn-primary btn-link cust-btn mb-2" id="btnRefresh" style="float:right;border: 0px;color: #fff; background-color:#337AB7; border-color: #3C8DBC; border-radius: 4px;"> <i class="fa-solid fa-arrows-rotate"></i>Refresh</button>
                            <div id="loader" style="display:none;"></div>
                            <div class="tblPartyList">
                            </div>

                        </div>
                    </div>
                </div>
               
            </div>
        </div>
    </section>
</div>
<!--Modal Add Party Type-->
<div class="modal fade" id="modal-add-party-type">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Add New Party Type</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Party Type</label>
                            <input type="text" class="form-control" id="" name="mdlPartyTypeAdd">
                        </div>
                    </div>

                </div>
            </div>
            <div class="modal-footer justify-content-between">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary PartyTypeAdd">Save changes</button>
            </div>

        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!--.end Modal Add Group-->
<!--Modal Update Party Type-->
<div class="modal fade" id="modal-edit-party-type">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Update Party Type</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div class="row">
                    <input type="hidden" name="mdlPartyTypeId" />
                    <div class="col-md-12">
                        <div class="form-group">
                            <label>Party Type</label>
                            <input type="text" class="form-control" id="" name="mdlPartyTypeEdit">
                        </div>
                    </div>

                </div>
            </div>
            <div class="modal-footer justify-content-between">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" class="btn btn-primary PartyTypeEdit">Save changes</button>
            </div>

        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>
<!--.end Modal Party Type-->
